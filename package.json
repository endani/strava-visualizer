    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "autoprefixer": "^10.0.2",
    "axios": ">=0.21.1",
    "dotenv": "^16.0.0",
    "eslint": "^7.0.0",
    "jest": "^27.4.5",
    "lodash": "^4.17.20",
    "mapbox-gl": "^v2.7.0",
    "moment": "^2.29.1",
    "postcss": "^8.1.10",
    "prop-types": "^15.7.2",
    "query-string": "^7.1.1",
    "react": "^16.14.0",
    "react-dom": "^17.0.1",
    "react-moment": "^1.0.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "recharts": "^2.0.0-beta.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.1",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.1.2",
