{
  "name": "strava-visualizer",
  "version": "0.0.0-semantic-released",
  "private": true,
  "scripts": {
    "build": "npm run watch:css && craco build",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "start": "npm run watch:css && craco start",
    "test": "jest --passWithNoTests",
    "watch:css": "postcss src/assets/tailwind.css -o src/assets/main.css",
    "semantic-release": "semantic-release"
  },
  "prettier": "@typeform/eslint-config/prettier",
  "dependencies": {
    "@mapbox/polyline": "^1.1.0",
    "@typeform/eslint-config": "^3.0.2",
    "@types/jest": "^28.1.1",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^17.0.14",
    "autoprefixer": "^10.0.2",
    "axios": ">=0.21.1",
    "dotenv": "^16.0.0",
    "eslint": "^7.0.0",
    "lodash": "^4.17.20",
    "mapbox-gl": "^v2.12.0",
    "moment": "^2.29.1",
    "postcss": "^8.1.10",
    "prop-types": "^15.7.2",
    "query-string": "^8.1.0",
    "react": "^16.14.0",
    "react-dom": "^18.1.0",
    "react-moment": "^1.0.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.1",
    "recharts": "^2.0.0-beta.1",
    "styled-components": "^5.2.1",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.1.2",
    "use-media": "^1.4.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@types/lodash": "^4.14.182",
    "babel-eslint": "^10.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-wesbos": "^3.0.2",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "jest": "^28.0.3",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.6.0",
    "react-query": "^3.34.19",
    "react-query-devtools": "^2.6.3",
    "tailwind-styled-components": "^2.1.4",
    "semantic-release": "^21.0.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings 0",
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      }
    ],
    "verifyConditions": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ],
    "tagFormat": "v${version}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/endani/strava-visualizer.git"
  }
}
